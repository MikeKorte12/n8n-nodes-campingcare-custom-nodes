{
	"name": "n8n-nodes-campingcare-custom-nodes",
	"version": "0.1.1",
	"description": "Custom n8n nodes for integrating with the Starfish API (CampingCare/HotelCare) - manage contacts, reservations, accommodations, and webhooks",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"starfish",
		"camping",
		"campingcare",
		"hotelcare",
		"reservations",
		"contacts",
		"webhooks",
		"api"
	],
	"license": "MIT",
	"homepage": "https://www.camping.care",
	"author": {
		"name": "Mike",
		"email": "mike@camping.care"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mikecampingcare/n8n-nodes-campingcare-custom-nodes.git"
	},
	"engines": {
		"node": ">=20.15"
	},
	"main": "index.js",
	"scripts": {
		"build": "npx rimraf dist && tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/CampingCareApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/CampingCare/CampingCare.node.js",
			"dist/nodes/CampingCare/CampingCareTrigger.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "~8.32.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
